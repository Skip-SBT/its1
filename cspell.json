  "ignorePaths": [
    "**/composer.json",
    "**/package.json",
    "src/translation"
  ],
  "import": [
    "@cspell/dict-de-ch/cspell-ext.json",
    "@cspell/dict-de-de/cspell-ext.json"
  ],
  "dictionaries": [
    "aws",
