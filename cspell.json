{
  "language": "en,de",
  "useGitignore": true,
  "files": [
    "**/*.bat",
    "**/*.css",
    "**/*.html",
    "**/*.js",
    "**/*.json",
    "**/*.jsx",
    "**/*.md",
    "**/*.php",
    "**/*.scss",
    "**/*.sh",
    "**/*.sql",
    "**/*.svg",
    "**/*.tmpl",
    "**/*.ts",
    "**/*.tsx",
    "**/*.twig",
    "**/*.txt",
    "**/*.xml",
    "**/*.xsd",
    "**/*.yaml",
    "**/*.yml",
    "**/Dockerfile",
    "**/Dockerfile.*"
